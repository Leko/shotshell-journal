{
  "expo": {
    "name": "実包等管理帳簿",
    "slug": "tmp",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "jp.leko.shellshot-journal",
      "config": {
        "googleSignIn": {
          "reservedClientId": "com.googleusercontent.apps.630184827238-ks3mlsbb4c0osu4tpeslao3alatn2ffc"
        }
      }
    },
    "android": {
      "package": "jp.leko.shellshotjournal",
      "googleServicesFile": "./src/firebase/google-services.json"
    }
  }
}
